<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="150.0" prefWidth="225.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="com.goodsforpets.desktop.controllers.AddManufacturerSceneController">
  <children>
    <VBox alignment="CENTER" layoutX="43.0" layoutY="-15.0" spacing="10.0" AnchorPane.bottomAnchor="0.0"
      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <TextField fx:id="manufacturerNameTextField" alignment="TOP_LEFT" promptText="Введите название производителя">
          <VBox.margin>
            <Insets left="5.0" right="5.0" top="10.0"/>
          </VBox.margin>
          <font>
            <Font name="Comic Sans MS" size="12.0"/>
          </font>
        </TextField>
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#onAddManufacturerButtonClick" text="Сохранить">
          <font>
            <Font name="Comic Sans MS" size="12.0"/>
          </font>
        </Button>
      </children>
    </VBox>
  </children>
</AnchorPane>
